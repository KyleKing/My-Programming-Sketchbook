<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Actions</key>
	<array>
		<dict>
			<key>Requirements</key>
			<array>
				<string>email</string>
			</array>
			<key>Script Interpreter</key>
			<string>/usr/bin/php</string>
			<key>Shell Script File</key>
			<string>domain.php</string>
			<key>Title</key>
			<string>@domain</string>
		</dict>
	</array>
	<key>Extension Description</key>
	<string>Open the domain of the email address as a web address.</string>
	<key>Extension Identifier</key>
	<string>com.pilotmoon.popclip.extension.email-domain</string>
	<key>Extension Name</key>
	<string>Email Domain</string>
</dict>
</plist>
