{
  "name": "onoff-demo",
  "version": "0.0.1",
  "description": "Runs a process, until a button is pressed, then quits process and shutdowns Raspberry Pi ",
  "main": "init.es6",
  "scripts": {
    "start": "node init.es6",
    "test": "./node_modules/eslint/bin/eslint.js . --ext .js,.jsx,.es6"
  },
  "author": "Kyle King",
  "license": "MIT",
  "dependencies": {
    "cron": "^1.1.0",
    "fs-extra": "^0.30.0",
    "moment": "^2.14.1",
    "onoff": "^1.1.1",
    "shelljs": "^0.7.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "pre-commit": "^1.1.3"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "no-unused-vars": [
        "warn"
      ],
      "curly": [
        "error",
        "multi"
      ],
      "no-console": [
        "off"
      ],
      "react/require-extension": [
        0
      ],
      "import/extensions": [
        1
      ]
    },
    "parser": "babel-eslint"
  }
}
