{
	"manifest_version": 2,

  "name": "Web CLock UX",
  "description": "Stores last clock action and only displays the opposite action.",
  "version": "0.3",

	"icons": {
			"40": "images/logo@0.5x.png",
			"80": "images/logo@1x.png",
			"160": "images/logo.png"
	},

	"browser_action": {
			"default_title": "browser_action: default_title",
      "default_icon": "images/logo@0.5x.png",
      "default_title": "WebClock UX",
			"default_popup": "popup/popup.html"
	},

  "permissions": [
    "storage"
  ],

	"content_scripts": [ {
		"js": [ "lib/jquery-2.1.4.min.js", "background.js", "lib/waves.min.js" ],
		"css": ["modifications.css", "lib/waves.min.css" ],
		"matches": [ "file:///Users/kyleking/Downloads/Web%20Clock.html", "https://*/workforce/WebClock.do*" ]
	}],

	"homepage_url": "http://kyleking.github.io"
}
